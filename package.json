{
  "name": "gm-react-app",
  "description": "参考资料",
  "version": "1.0.0",
  "main": "webpack.config.backup.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmfe/gm-react-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gmfe/gm-react-app/issues"
  },
  "homepage": "https://github.com/gmfe/gm-react-app#readme",
  "scripts": {
    "start": "gm-react-app-scripts start",
    "build": "gm-react-app-scripts build",
    "publish-latest": "lerna publish --force-publish",
    "publish-beta": "lerna publish --force-publish --dist-tag beta",
    "autolink": "cd packages;cd babel-preset-gm-react-app; yarn link; cd ../eslint-plugin-gm-react-app; yarn link; cd ../scripts; yarn link; cd ..; yarn link @gm-react-app/scripts eslint-plugin-gm-react-app babel-preset-gm-react-app"
  },
  "lint-staged": {
    "./src/**/*.{js,json}": [
      "eslint --cache --fix",
      "git add"
    ],
    "./src/**/*.{less,css}": [
      "stylelint --cache --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@gm-react-app/scripts": "0.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-jsx": "^3.2.4"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "lerna": "^3.19.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0"
  },
  "browserslist": {
    "production": [
      "iOS >= 8",
      "Android >= 5.0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

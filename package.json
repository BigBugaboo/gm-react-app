{
  "name": "gm-react-app",
  "description": "参考资料",
  "version": "1.0.0",
  "main": "webpack.config.backup.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmfe/gm-react-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gmfe/gm-react-app/issues"
  },
  "homepage": "https://github.com/gmfe/gm-react-app#readme",
  "scripts": {
    "publish-latest": "lerna publish --force-publish",
    "publish-beta": "lerna publish --force-publish --dist-tag beta",
    "autolink": "cd packages;cd babel-preset-gm-react-app; yarn link; cd ../eslint-plugin-gm-react-app; yarn link; cd ../scripts; yarn link; cd ..; yarn link @gm-react-app/scripts eslint-plugin-gm-react-app babel-preset-gm-react-app",
    "start": "gras start",
    "build:test": "gras test",
    "build:prod": "gras build"
  },
  "lint-staged": {
    "./src/**/*.{js,json}": [
      "eslint --cache --fix",
      "git add"
    ],
    "./src/**/*.{less,css}": [
      "stylelint --cache --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@gm-react-app/scripts": "^1.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "lerna": "^3.19.0"
  },
  "browserslist": {
    "production": [
      "iOS >= 8",
      "Android >= 5.0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
